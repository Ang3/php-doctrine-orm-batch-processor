{
  "name": "ang3/php-doctrine-orm-batch-processor",
  "type": "library",
  "homepage": "https://github.com/Ang3/php-doctrine-orm-batch-processor",
  "description": "Batch processor for Doctrine ORM entities",
  "license": "proprietary",
  "authors": [
    {
      "name": "Joanis ROUANET",
      "email": "joanis.ang3@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {"Ang3\\Component\\Doctrine\\ORM\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "Ang3\\Component\\Doctrine\\ORM\\Tests\\": "tests/" }
  },
  "require": {
    "php": "^7.2.5",
    "doctrine/orm": "^2.7"
  },
  "config": {
    "sort-packages": true
  },
  "require-dev": {
    "symfony/var-dumper": "^5.1"
  }
}
